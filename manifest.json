 {
    "name": "ICFactChecker",
    "version": "1.0",
    "description": "Check yo facts, yo",
  	"background": {
      "scripts": ["background.js", "jquery-3.5.1.js"],
      "persistent": false
    },

    "icons": { "16": "ogsmall.png",
           "48": "ogmedium.png",
          "128": "oglarge.png" },

    "browser_action": {
   	"default_icon": "ogsmall.png",
   	"default_popup": "popup.html"},
    "permissions" : ["declarativeContent","activeTab","storage", "contextMenus"],
    "web_accessible_resources" : ["infoPopup.html", "infoPopup.js", "infoDisplay.js"],
    "manifest_version": 2
  }
